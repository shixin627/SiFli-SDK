from building import *
import rtconfig

# get current directory
cwd = GetCurrentDir()
# The set of source files associated with this SConscript file.
src = Glob('*.c')

path = [cwd + '/']

LOCAL_CCFLAGS = ''

group = DefineGroup('kraepelin_reference', src, depend = ['PKG_USING_KRAEPELIN_REFERENCE'], CPPPATH = path, LOCAL_CCFLAGS = LOCAL_CCFLAGS)

Return('group')
